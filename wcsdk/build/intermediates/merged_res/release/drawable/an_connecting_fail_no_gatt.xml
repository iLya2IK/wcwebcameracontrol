<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:drawable="@drawable/ic_connected">
    <target
        android:name="male_body">
        <aapt:attr name="android:animation">
            <set android:ordering="sequentially">
                <objectAnimator
                    android:interpolator="@android:anim/accelerate_interpolator"
                    android:duration="300"
                    android:valueFrom="0.0"
                    android:valueTo="250.0"
                    android:valueType="floatType"
                    android:propertyName="translateX"/>
                <objectAnimator
                    android:interpolator="@android:anim/accelerate_interpolator"
                    android:duration="300"
                    android:valueFrom="250.0"
                    android:valueTo="0.0"
                    android:valueType="floatType"
                    android:propertyName="translateX"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="male_wire">
        <aapt:attr name="android:animation">
            <set android:ordering="sequentially">
                <objectAnimator
                    android:propertyName="pathData"
                    android:duration="300"
                    android:valueFrom="M1100,800L1500,800"
                    android:valueTo="M1100,800L1850,800"
                    android:valueType="pathType"
                    android:interpolator="@android:anim/linear_interpolator"/>
                <objectAnimator
                    android:propertyName="pathData"
                    android:duration="300"
                    android:valueFrom="M1100,800L1850,800"
                    android:valueTo="M1100,800L1500,800"
                    android:valueType="pathType"
                    android:interpolator="@android:anim/linear_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="male_body_fill">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:duration="400"
                android:valueFrom="0.0"
                android:valueTo="0.0"
                android:valueType="floatType"
                android:interpolator="@android:anim/linear_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="female_body_fill">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:duration="400"
                android:valueFrom="0.0"
                android:valueTo="0.0"
                android:valueType="floatType"
                android:interpolator="@android:anim/linear_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="female_body_stroke">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="strokeAlpha"
                android:duration="400"
                android:valueFrom="1.0"
                android:valueTo="0.0"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="female_wire">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="strokeAlpha"
                android:duration="400"
                android:valueFrom="1.0"
                android:valueTo="0.0"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="halt_ico">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="fillAlpha"
                android:duration="600"
                android:valueFrom="0.0"
                android:valueTo="1.0"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="halt_ico_group">
        <aapt:attr name="android:animation">
            <set  android:ordering="sequentially">
                <set android:ordering="together">
                    <objectAnimator
                        android:propertyName="translateX"
                        android:duration="100"
                        android:valueFrom="0.0"
                        android:valueTo="350"
                        android:valueType="floatType"
                        android:interpolator="@android:anim/accelerate_interpolator"/>
                    <objectAnimator
                        android:propertyName="scaleX"
                        android:duration="500"
                        android:valueFrom="0.05"
                        android:valueTo="1.0"
                        android:valueType="floatType"
                        android:interpolator="@android:anim/accelerate_interpolator"/>
                    <objectAnimator
                        android:propertyName="scaleY"
                        android:duration="500"
                        android:valueFrom="0.05"
                        android:valueTo="1.0"
                        android:valueType="floatType"
                        android:interpolator="@android:anim/accelerate_interpolator"/>
                </set>
                <objectAnimator
                    android:propertyName="rotation"
                    android:duration="80"
                    android:repeatCount="infinite"
                    android:repeatMode="reverse"
                    android:valueFrom="0.0"
                    android:valueTo="15.0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>